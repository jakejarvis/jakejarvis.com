/* symbol search results */
div.wsod_symbolSearch { position:absolute; background:#fff; z-index:9900000; padding:0; border:1px solid #ccc; border-right-color:#666; border-bottom-color:#666; /*-moz-box-shadow: #a1a1a1 2px 2px 5px;*/ }

div.wsod_symbolSearch table { padding:0; margin:0; border-collapse:collapse; }
div.wsod_symbolSearch td { text-align:left; color: #333; font-size:11px; padding:6px 5px; cursor:pointer; border-bottom:1px solid #e1e1e1; }
div.wsod_symbolSearch td.wsod_sym { text-align:right; }
div.wsod_symbolSearch tr.resultType td { font-size:10px; font-weight:bold; color:#012950; cursor:text !important; }
div.wsod_symbolSearch tr.resultType img { padding-bottom:2px; }
div.wsod_symbolSearch tr.nameMatches td { padding-top:13px; }
div.wsod_symbolSearch tr.last,
div.wsod_symbolSearch tr.last td { border-bottom:none; }
div.wsod_symbolSearch .hover { background: #eeeeee; text-decoration: none; }
div.wsod_symbolSearch div.wsod_flag { margin-right:1px; }

.wsod_flag { margin:0; height:11px; width:16px;background:url(https://i.cdn.turner.com/money/.element/img/3.0/data/country-flag.bg.gif) no-repeat 50px 50px;overflow:hidden;}

