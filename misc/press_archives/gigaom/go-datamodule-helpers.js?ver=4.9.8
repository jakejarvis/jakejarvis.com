var go_datamodule = {};

( function( $ ) {
	'use strict';

	// Add commas to a number
	go_datamodule.add_commas = function( number ) {
		number += '';

		var x = number.split('.');
		var x1 = x[0];
		var x2 = x.length > 1 ? '.' + x[1] : '';
		var regex = /(\d+)(\d{3})/;

		while ( regex.test( x1 ) ) {
			x1 = x1.replace( regex, '$1' + ',' + '$2') ;
		}

		return x1 + x2;
	};
})( jQuery );